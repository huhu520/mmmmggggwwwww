<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        android:padding="10dp"
        android:visibility="gone" >

  <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/iv_item_home_food"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:background="@drawable/btn_white_click"
                android:scaleType="centerCrop"
                android:src="@drawable/select_food_bg" />

            <TextView
                android:id="@+id/tv_item_home_01"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="美食"
                android:textSize="15sp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/iv_item_home_fastfood"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:background="@drawable/btn_white_click"
                android:scaleType="centerCrop"
                android:src="@drawable/select_fastfoot_bg" />

            <TextView
                android:id="@+id/tv_item_home_02"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="小吃快餐"
                android:textSize="15sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/iv_item_home_fun"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:background="@drawable/btn_white_click"
                android:scaleType="centerCrop"
                android:src="@drawable/select_fun_bg" />

            <TextView
                android:id="@+id/tv_item_home_03"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="休闲娱乐"
                android:textSize="15sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical" >

            <ImageView
                android:id="@+id/iv_item_home_more"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:background="@drawable/btn_white_click"
                android:scaleType="centerCrop"
                android:src="@drawable/select_more_bg" />

            <TextView
                android:id="@+id/tv_item_home_04"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="更多"
                android:textSize="15sp" />
        </LinearLayout>
    </LinearLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/vp_home_options"
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_gravity="center"
        android:visibility="gone" >

      
    </android.support.v4.view.ViewPager>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="20dp"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/viewGroup"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="40dp"
                android:gravity="center_horizontal"
                android:orientation="horizontal" >
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_home_points"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        android:visibility="gone" >

        <ImageView
            android:id="@+id/iv_home_point01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/dot_s"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/iv_home_point02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:background="@drawable/dot"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/iv_home_point03"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:background="@drawable/dot"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/iv_home_point04"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:background="@drawable/dot"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/iv_home_point05"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:background="@drawable/dot"
            android:visibility="gone" />
    </LinearLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="#CCCCCC" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="90dp" >

        <RelativeLayout
            android:id="@+id/bt_home_coupon"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="5dp"
            android:layout_weight="1" >

            <ImageView
                android:id="@+id/iv_getcoupon_smile"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="5dp"
                android:src="@drawable/bt_home_couponb" />
        </RelativeLayout>

        <View
            android:layout_width="1px"
            android:layout_height="fill_parent"
            android:layout_marginBottom="2dp"
            android:layout_marginTop="2dp"
            android:background="#CCCCCC" />

        <RelativeLayout
            android:id="@+id/bt_home_seckill"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="5dp"
            android:layout_weight="1" >

            <ImageView
                android:id="@+id/iv_msgt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:src="@drawable/bt_home_seckill" />
        </RelativeLayout>
    </LinearLayout>

    <View
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="#CCCCCC" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="12dp"
        android:background="#e4e4e4" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:drawableLeft="@drawable/ic_loc_0"
        android:drawablePadding="10dp"
        android:gravity="center_vertical"
        android:padding="10dip"
        android:paddingLeft="10dp"
        android:text="距离你最近的店铺"
        android:textColor="#727272"
        android:textSize="14sp" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="1px"
        android:background="#CCCCCC" />

</LinearLayout>